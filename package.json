{
  "name": "@elastic/transport",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "types": "lib/",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "npm run lint && tap test/{unit,acceptance}/{*,**/*}.test.js",
    "test:unit": "npm run build && tap test/unit/{*,**/*}.test.ts",
    "test:acceptance": "tap test/acceptance/*.test.js",
    "test:coverage-100": "tap test/{unit,acceptance}/{*,**/*}.test.js --coverage --100",
    "test:coverage-report": "tap test/{unit,acceptance}/{*,**/*}.test.js --coverage && nyc report --reporter=text-lcov > coverage.lcov",
    "test:coverage-ui": "tap test/unit/{*,**/*}.test.ts --coverage --coverage-report=html",
    "lint": "ts-standard src",
    "lint:fix": "ts-standard --fix src",
    "license-checker": "license-checker --production --onlyAllow='MIT;Apache-2.0;Apache1.1;ISC;BSD-3-Clause;BSD-2-Clause'",
    "prebuild": "npm run clean-build && npm run lint",
    "build": "tsc",
    "clean-build": "rimraf ./lib && mkdir lib",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elastic/elastic-transport-js.git"
  },
  "keywords": [],
  "author": {
    "name": "Tomas Della Vedova",
    "company": "Elastic BV"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/elastic/elastic-transport-js/issues"
  },
  "homepage": "https://github.com/elastic/elastic-transport-js#readme",
  "engines": {
    "node": ">=12"
  },
  "devDependencies": {
    "@sinonjs/fake-timers": "github:sinonjs/fake-timers#0bfffc1",
    "@types/debug": "^4.1.5",
    "@types/ms": "^0.7.31",
    "@types/node": "^14.14.31",
    "@types/stoppable": "^1.1.0",
    "@types/tap": "^14.10.2",
    "into-stream": "^6.0.0",
    "license-checker": "^25.0.1",
    "node-abort-controller": "^1.1.0",
    "proxy": "^1.0.2",
    "rimraf": "^3.0.2",
    "standard": "^16.0.3",
    "stoppable": "^1.1.0",
    "tap": "^14.11.0",
    "ts-node": "^9.1.1",
    "ts-standard": "^10.0.0",
    "typescript": "^4.1.5",
    "workq": "^3.0.0"
  },
  "dependencies": {
    "debug": "^4.3.1",
    "hpagent": "^0.1.1",
    "ms": "^2.1.3",
    "secure-json-parse": "^2.3.1",
    "tslib": "^2.1.0",
    "undici": "^3.3.3"
  },
  "tap": {
    "esm": false,
    "ts": true,
    "jsx": false,
    "flow": false,
    "coverage": false
  }
}
